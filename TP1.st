Object subclass: #Condicion	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!Condicion methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 20:47'!permisoCircular	^DefectoCircular new.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Condicion class	instanceVariableNames: ''!!Condicion class methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 23:47'!segun: sintomas 	sintomas ifEmpty: [ ^CovidNegativo new ].	(sintomas size > 3 )ifTrue: [ ^CovidPositivo new ].	(sintomas size = 3 )ifTrue: [ 		 ( sintomas intersection: #('Tos' 'Fiebre' 'Cansancio') )ifNotEmpty:[ ^CovidPositivo new.]	].	^ SospechosoCovid new.! !Condicion subclass: #ContactoEstrechoNegativo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!Condicion subclass: #ContactoEstrechoPositivo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!ContactoEstrechoPositivo methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 21:22'!permisoCircular 	^NoPuedeCircular new.! !Condicion subclass: #ContactoEstrechoSospechoso	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!ContactoEstrechoSospechoso methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 21:22'!permisoCircular 	^NoPuedeCircular new.! !Condicion subclass: #CovidNegativo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!CovidNegativo methodsFor: 'grupos' stamp: 'NicolasZulaica 5/13/2021 23:18'!pinchaBurbuja: burbuja	^nil.! !!CovidNegativo methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 20:12'!diagnostico 	^'Negativo'.! !!CovidNegativo methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 22:27'!impactaA: persona	^nil.! !!CovidNegativo methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 20:12'!estadoAlContactar	^ContactoEstrechoNegativo new.! !Condicion subclass: #CovidPositivo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!CovidPositivo methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 20:12'!diagnostico 	^'Positivo'.! !!CovidPositivo methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 20:04'!estadoAlContactar	^ContactoEstrechoPositivo new.! !!CovidPositivo methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 22:28'!impactaA: persona 	persona estaComprometida.! !!CovidPositivo methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 21:22'!permisoCircular 	^NoPuedeCircular new.! !!CovidPositivo methodsFor: 'grupos' stamp: 'NicolasZulaica 5/13/2021 23:17'!pinchaBurbuja: burbuja 	burbuja pincharBurbuja.! !Condicion subclass: #EnBurbujaPinchada	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!EnBurbujaPinchada methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 22:42'!permisoCircular 	^NoPuedeCircular new.! !Condicion subclass: #NoVacunado	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!Condicion subclass: #PersonalDeRiesgo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!PersonalDeRiesgo methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 21:22'!permisoCircular 	^NoPuedeCircular new.! !Condicion subclass: #PersonalEscencial	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!PersonalEscencial methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 21:22'!permisoCircular 	^PuedeCircular new.! !Condicion subclass: #SospechosoCovid	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!SospechosoCovid methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 20:12'!diagnostico 	^'Sospechoso'.! !!SospechosoCovid methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 20:06'!estadoAlContactar	^ContactoEstrechoSospechoso new! !!SospechosoCovid methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 22:27'!impactaA: persona 	^nil.! !!SospechosoCovid methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 20:52'!permisoCircular	^NoPuedeCircular new.! !!SospechosoCovid methodsFor: 'grupos' stamp: 'NicolasZulaica 5/13/2021 23:19'!pinchaBurbuja: burbuja	^nil.! !Condicion subclass: #Vacunado	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Condiciones'!!Vacunado methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 20:52'!permisoCircular	^PuedeCircular new.! !Object subclass: #AlgoVid	instanceVariableNames: 'personas burbujas'	classVariableNames: ''	package: 'TP1-Main'!!AlgoVid methodsFor: 'personas' stamp: 'NicolasZulaica 5/10/2021 22:10'!puedeCircular: nombre	^ (personas at: nombre ifAbsent: [^nil] ) puedeCircular.! !!AlgoVid methodsFor: 'personas' stamp: 'NicolasZulaica 5/11/2021 23:15'!diagnosticoDe: persona 	^ (personas at: persona) diagnostico.! !!AlgoVid methodsFor: 'colegio' stamp: 'NicolasZulaica 5/13/2021 23:32'!clasesPresencialesEn: nombreColegio 	^false.! !!AlgoVid methodsFor: 'colegio' stamp: 'NicolasZulaica 5/13/2021 23:34'!agregarA: nombrePersona alColegio: nombreColegio ! !!AlgoVid methodsFor: 'colegio' stamp: 'NicolasZulaica 5/13/2021 23:32'!nuevoColegio: nombre  ! !!AlgoVid methodsFor: 'personas-contacto' stamp: 'NicolasZulaica 5/13/2021 20:11'!contactoEstrechoEntre: nombre1 y: nombre2 	(personas at: nombre1) enConactoCon: (personas at: nombre2).! !!AlgoVid methodsFor: 'initialization' stamp: 'NicolasZulaica 5/13/2021 22:55'!initialize	personas := Dictionary new.	burbujas := Dictionary new.! !!AlgoVid methodsFor: 'personas-condiciones' stamp: 'NicolasZulaica 5/13/2021 20:19'!esPersonalEsencial: nombre 	(personas at: nombre) esPersonalEsencial.! !!AlgoVid methodsFor: 'personas-condiciones' stamp: 'NicolasZulaica 5/13/2021 21:04'!agregarSintoma: sintoma a: nombrePersona 	^(personas at: nombrePersona ifAbsent: [^nil] ) agregarSintoma: sintoma.! !!AlgoVid methodsFor: 'personas-condiciones' stamp: 'NicolasZulaica 5/10/2021 18:10'!nuevaPersona: nombre	personas at:nombre put: Persona new.! !!AlgoVid methodsFor: 'personas-condiciones' stamp: 'NicolasZulaica 5/13/2021 20:19'!esPersonaDeRiesgo: nombre 	(personas at: nombre) esPersonaDeRiesgo.! !!AlgoVid methodsFor: 'personas-condiciones' stamp: 'NicolasZulaica 5/10/2021 22:10'!estaVacunadx: nombre	(personas at: nombre ifAbsent: [^nil] ) estaVacunadx.! !!AlgoVid methodsFor: 'burbujas' stamp: 'NicolasZulaica 5/13/2021 23:00'!nuevaBurbuja: nombre 	burbujas at: nombre put: Burbuja new.! !!AlgoVid methodsFor: 'burbujas' stamp: 'NicolasZulaica 5/13/2021 23:02'!burbujaPinchada: nombreBurbuja  	^(burbujas at: nombreBurbuja) estaPinchada.! !!AlgoVid methodsFor: 'burbujas' stamp: 'NicolasZulaica 5/13/2021 23:34'!agregarA: nombrePersona aLaBurbuja: nombreBurbuja 	( burbujas at: nombreBurbuja ) agregar: ( personas at: nombrePersona ).! !Object subclass: #Burbuja	instanceVariableNames: 'personas estado'	classVariableNames: ''	package: 'TP1-Main'!!Burbuja methodsFor: 'initialization' stamp: 'NicolasZulaica 5/13/2021 21:54'!initialize	personas := Set new.	estado := EstadoNoPinchada new.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 22:54'!agregar: persona 	personas add: persona.	persona agregarGrupo: self.	estado efectoDe: self alAgregar: persona.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 21:54'!estaPinchada	^estado estaPinchada.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 22:44'!pincharBurbuja	estado := EstadoPinchada new.	personas do: [ :persona | persona enBurbujaPinchada ].! !Object subclass: #Persona	instanceVariableNames: 'condiciones sintomas grupos'	classVariableNames: ''	package: 'TP1-Main'!!Persona methodsFor: 'acceso' stamp: 'NicolasZulaica 5/11/2021 23:22'!diagnostico	^(condiciones at: 'covid') diagnostico.! !!Persona methodsFor: 'acceso' stamp: 'NicolasZulaica 5/13/2021 20:10'!puedeCircular	^ (condiciones inject: DefectoCircular new into:[ :acumulado :condicion | acumulado y: (condicion permisoCircular )  ]) puedeCircular.! !!Persona methodsFor: 'grupos' stamp: 'NicolasZulaica 5/13/2021 23:16'!pinchaBurbuja: burbuja	(condiciones at: 'covid') pinchaBurbuja: burbuja.! !!Persona methodsFor: 'grupos' stamp: 'NicolasZulaica 5/13/2021 22:15'!agregarGrupo: grupo	grupos add: grupo.! !!Persona methodsFor: 'agregarCondicion' stamp: 'NicolasZulaica 5/13/2021 20:10'!estaVacunadx	condiciones at: 'vacunado' put: Vacunado new.! !!Persona methodsFor: 'agregarCondicion' stamp: 'NicolasZulaica 5/13/2021 20:17'!esPersonaDeRiesgo	condiciones at: 'riesgo' put: PersonalDeRiesgo new.! !!Persona methodsFor: 'agregarCondicion' stamp: 'NicolasZulaica 5/11/2021 23:49'!esPersonalEsencial	condiciones at:'escencial' put: PersonalEscencial new.! !!Persona methodsFor: 'agregarCondicion' stamp: 'NicolasZulaica 5/13/2021 22:34'!estaComprometida	grupos do: [ :grupo | grupo pincharBurbuja ].! !!Persona methodsFor: 'modificarCondicion' stamp: 'NicolasZulaica 5/13/2021 19:36'!enConactoCon: otraPersona 	self contactar: otraPersona.	otraPersona contactar: self. ! !!Persona methodsFor: 'modificarCondicion' stamp: 'NicolasZulaica 5/13/2021 22:19'!agregarSintoma: sintoma 	sintomas add: sintoma.	self actualizarCovid: (Condicion segun: sintomas).! !!Persona methodsFor: 'initialization' stamp: 'NicolasZulaica 5/13/2021 22:32'!initialize	condiciones := Dictionary new.	sintomas := Set new.	grupos := Set new.		condiciones at: 'vacunado' put: NoVacunado new.	self actualizarCovid: CovidNegativo new.! !!Persona methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 22:46'!enBurbujaPinchada	condiciones at: 'burbuja' put: EnBurbujaPinchada new.! !!Persona methodsFor: 'private' stamp: 'NicolasZulaica 5/13/2021 19:44'!contactar: otraPersona	condiciones at:'contacto' put:		otraPersona estadoAlContactar. ! !!Persona methodsFor: 'private' stamp: 'NicolasZulaica 5/13/2021 22:26'!actualizarCovid: condicion	condiciones at: 'covid' put: condicion.	condicion impactaA: self.! !!Persona methodsFor: 'private' stamp: 'NicolasZulaica 5/13/2021 20:10'!estadoAlContactar	^(condiciones at: 'covid') estadoAlContactar.! !Object subclass: #DefectoCircular	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Permisos'!!DefectoCircular methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/10/2021 21:50'!puedeCircular 	^false.! !!DefectoCircular methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 20:32'!y: otroPermiso  	^ otroPermiso.! !Object subclass: #NoPuedeCircular	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Permisos'!!NoPuedeCircular methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/10/2021 20:45'!puedeCircular	^false.! !!NoPuedeCircular methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 20:32'!y: otroPermiso  	^self.! !Object subclass: #PuedeCircular	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Permisos'!!PuedeCircular methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/10/2021 18:47'!puedeCircular	^true.! !!PuedeCircular methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/11/2021 20:33'!y: otroPermiso	(otroPermiso class = NoPuedeCircular )ifTrue: [ ^otroPermiso ].	^self.! !Object subclass: #EstadoNoPinchada	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-EstadosBurbuja'!!EstadoNoPinchada methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 23:13'!efectoDe: burbuja alAgregar: persona.	persona pinchaBurbuja: burbuja.! !!EstadoNoPinchada methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 21:51'!estaPinchada	^false.! !Object subclass: #EstadoPinchada	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-EstadosBurbuja'!!EstadoPinchada methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 22:54'!efectoDe: burbuja alAgregar: persona.	persona enBurbujaPinchada.! !!EstadoPinchada methodsFor: 'as yet unclassified' stamp: 'NicolasZulaica 5/13/2021 21:52'!estaPinchada	^true! !